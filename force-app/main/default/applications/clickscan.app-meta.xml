<?xml version="1.0" encoding="UTF-8"?>
<CustomApplication xmlns="http://soap.sforce.com/2006/04/metadata">
    <label>Clickscan</label>
    <uiType>Lightning</uiType>
    <navType>Standard</navType>
    <formFactors>Large</formFactors>
    <!-- Home page action override -->
    <actionOverrides>
        <actionName>Home</actionName>
        <comment>Home page override to display dashboard.</comment>
        <content>sample</content> <!-- Replace with your Dashboard page API name -->
        <formFactor>Large</formFactor>
        <skipRecordTypeSelect>false</skipRecordTypeSelect>
        <type>Flexipage</type>
    </actionOverrides>

    <!-- App pages action overrides -->
    <actionOverrides>
        <actionName>View</actionName>
        <comment>Action override for App Page 1.</comment>
        <content>Drawer_Management</content> <!-- Replace with your actual app page API name -->
        <formFactor>Large</formFactor>
        <skipRecordTypeSelect>false</skipRecordTypeSelect>
        <type>Flexipage</type>
        <pageOrSobjectType>Account</pageOrSobjectType>
    </actionOverrides>

    <actionOverrides>
        <actionName>View</actionName>
